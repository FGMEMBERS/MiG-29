<!-- Mig-29 9-13 Instruments            -->
<!-- Gary R. Neely 'Buckaroo', Jan 2009 -->


<PropertyList>

<!-- Special Groups and Shaders -->

<animation>
  <type>noshadow</type>
  <object-name>aoa</object-name>
  <object-name>asi</object-name>
  <object-name>alt</object-name>
  <object-name>gear_display</object-name>
  <object-name>ai</object-name>
  <object-name>mach</object-name>
  <object-name>rpm</object-name>
  <object-name>radio_alt</object-name>
  <object-name>eng_temp_L</object-name>
  <object-name>eng_temp_R</object-name>
  <object-name>clock</object-name>
  <object-name>control_positions</object-name>
  <object-name>fuel_display</object-name>
  <object-name>ramps</object-name>
</animation>


<model>
