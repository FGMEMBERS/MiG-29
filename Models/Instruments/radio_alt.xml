<?xml version="1.0"?>

<PropertyList>

<path>radio_alt.ac</path>

<animation>
  <type>material</type>
  <object-name>RadAlt.Face</object-name>
  <object-name>RadAlt.Needle</object-name>
  <emission>
    <red>0.8</red>
    <green>0.4</green>
    <blue>0.2</blue>
    <factor-prop>/controls/lighting/panel-norm</factor-prop>
  </emission>
</animation>

<animation>
  <type>rotate</type>
  <object-name>RadAlt.Needle</object-name>
  <property>/position/gear-agl-ft</property>
  <interpolation>
    <entry><ind>  0</ind><dep>  0</dep></entry>
    <entry><ind>300</ind><dep>150</dep></entry>
    <entry><ind>500</ind><dep>162.5</dep></entry>
    <entry><ind>1000</ind><dep>192.5</dep></entry>
    <entry><ind>3000</ind><dep>312.5</dep></entry>
    <entry><ind>60000</ind><dep>312.5</dep></entry>
  </interpolation>
  <axis>
    <x>-1</x>
    <y>0</y>
    <z>0</z>
  </axis>
</animation>

</PropertyList>

