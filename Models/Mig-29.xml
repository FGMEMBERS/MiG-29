<?xml version="1.0"?>

<!-- Mig-29 9-12A                         -->
<!-- Gary R. Neely 'Buckaroo', Nov 2008  -->

<PropertyList> 

<path>mesh_airframe.ac</path>


<!-- Sub-Model Imports -->

<model>
  <name>Cockpit</name>
  <path>Aircraft/Mig-29/Models/Mig-29_Cockpit.xml</path>
</model>
<model>
  <name>Exhaust_Nozzles</name>
  <path>Aircraft/Mig-29/Models/Mig-29_Nozzles.xml</path>
</model>
<model>
  <name>Reheat</name>
  <path>Aircraft/Mig-29/Models/Mig-29_Reheat.xml</path>
</model>
<model>
  <name>External_Lights</name>
  <path>Aircraft/Mig-29/Models/Mig-29_Lights.xml</path>
</model>

<!-- Level Of Detail -->

<animation>
  <type>range</type>
  <object-name>Cockpit</object-name>
  <min-m>0</min-m>
  <max-m>50</max-m>
</animation>
<!--
<animation>
  <type>range</type>
  <object-name>Antennas</object-name>
  <min-m>0</min-m>
  <max-m>150</max-m>
</animation>
-->

<!-- Special Shaders -->

<animation>
  <type>noshadow</type>
  <object-name>WheelGuard</object-name>
  <object-name>Canopy</object-name>
</animation>


<!-- Animations  -->


<!-- Rudders -->

<animation>
  <type>rotate</type>
  <object-name>RudderL</object-name>
  <property>surface-positions/rudder-pos-norm</property>
  <factor>-25</factor>
  <axis>
    <x1-m>14.042</x1-m>
    <y1-m>-1.806</y1-m>
    <z1-m>0.525</z1-m>
    <x2-m>14.376</x2-m>
    <y2-m>-1.922</y2-m>
    <z2-m>1.739</z2-m>
  </axis>
</animation>
<animation>
  <type>rotate</type>
  <object-name>RudderR</object-name>
  <property>surface-positions/rudder-pos-norm</property>
  <factor>-25</factor>
  <axis>
    <x1-m>14.042</x1-m>
    <y1-m>1.806</y1-m>
    <z1-m>0.525</z1-m>
    <x2-m>14.376</x2-m>
    <y2-m>1.922</y2-m>
    <z2-m>1.739</z2-m>
  </axis>
</animation>

<!-- Elevons -->

<animation>
  <type>rotate</type>
  <object-name>StabilizerL</object-name>
  <property>surface-positions/left-elevon-pos-norm</property>
  <factor>30</factor>
  <axis>
    <x1-m>13.488</x1-m>
    <y1-m>-1.833</y1-m>
    <z1-m>0.101</z1-m>
    <x2-m>13.488</x2-m>
    <y2-m>1.833</y2-m>
    <z2-m>0.101</z2-m>
  </axis>
</animation>

<animation>
  <type>rotate</type>
  <object-name>StabilizerR</object-name>
  <property>surface-positions/right-elevon-pos-norm</property>
  <factor>30</factor>
  <axis>
    <x1-m>13.488</x1-m>
    <y1-m>-1.833</y1-m>
    <z1-m>0.101</z1-m>
    <x2-m>13.488</x2-m>
    <y2-m>1.833</y2-m>
    <z2-m>0.101</z2-m>
  </axis>
</animation>

<!-- Ailerons -->
<!-- Note: The Mig-29 9-12 has an odd 5 deg up neutral position for ailerons, related -->
<!-- to better spin characteristics. Observable if one looks close at photos. -->

<animation>
  <type>rotate</type>
  <object-name>AileronL</object-name>
  <property>controls/flight/aileron</property>
  <factor>20</factor>
  <offset-deg>-5</offset-deg>
  <axis>
    <x1-m>12.062</x1-m>
    <y1-m>-5.065</y1-m>
    <z1-m>0.106</z1-m>
    <x2-m>11.656</x2-m>
    <y2-m>-3.715</y2-m>
    <z2-m>0.149</z2-m>
  </axis>
</animation>
<animation>
  <type>rotate</type>
  <object-name>AileronR</object-name>
  <property>controls/flight/aileron</property>
  <factor>20</factor>
  <offset-deg>5</offset-deg>
  <axis>
    <x1-m>12.062</x1-m>
    <y1-m>5.065</y1-m>
    <z1-m>0.106</z1-m>
    <x2-m>11.656</x2-m>
    <y2-m>3.715</y2-m>
    <z2-m>0.149</z2-m>
  </axis>
</animation>

